<%= form_with(model: @property, local: true) do |form| %>
  <div class="field">
    <%= form.label :owner %>
    <%= form.text_field :owner %>
  </div>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :property_type %>
    <%= form.text_field :property_type %>
  </div>

  <div class="actions">
    <%= form.submit "edit", class: "btn btn-primary" %>
  </div>
<% end %>
